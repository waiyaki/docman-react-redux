{
  "name": "docman",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "NODE_ENV=production node ./bin/www",
    "start:dev": "NODE_ENV=development nodemon ./bin/www",
    "test": "npm run test:bend",
    "test:bend": "NODE_ENV=testing istanbul cover _mocha test/server/**/*.js",
    "test:fend": "NODE_ENV=testing mocha --compilers js:babel-core/register --require ./test/helpers/fend-test-setup.js \"test/client/**/*@(.js|.jsx)\"",
    "test:fend:watch": "npm run test:fend -- --watch --watch-extensions jsx",
    "posttest": "npm run coverage",
    "coverage": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js"
  },
  "dependencies": {
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "~1.13.2",
    "debug": "~2.2.0",
    "dotenv": "^2.0.0",
    "express": "~4.13.1",
    "immutable": "^3.8.1",
    "jsonwebtoken": "^5.7.0",
    "material-ui": "^0.15.0",
    "mongoose": "^4.4.12",
    "morgan": "~1.6.1",
    "react": "^15.0.2",
    "react-dom": "^15.0.2",
    "react-router": "^2.4.0",
    "react-tap-event-plugin": "^1.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.8.0",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "chai": "^3.5.0",
    "chai-immutable": "^1.5.4",
    "coveralls": "^2.11.9",
    "eslint": "^2.10.1",
    "eslint-config-standard": "^5.3.1",
    "eslint-plugin-promise": "^1.1.0",
    "eslint-plugin-react": "^5.1.1",
    "eslint-plugin-standard": "^1.3.2",
    "istanbul": "^0.4.3",
    "jsdom": "^9.0.0",
    "mocha": "^2.4.5",
    "react-hot-loader": "^1.3.0",
    "supertest": "^1.2.0",
    "webpack": "^1.13.0",
    "webpack-dev-server": "^1.14.1"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  }
}
